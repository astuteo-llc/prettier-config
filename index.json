{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 80,
  "semi": false,
  "tabWidth": 4,
  "trailingComma": "all",
  "endOfLine": "lf",
  "overrides": [
      {
          "files": ["*.js","*.ts"],
          "options": {
            "singleQuote": true,
            "tabWidth": 2
          }
      }
  ]
}